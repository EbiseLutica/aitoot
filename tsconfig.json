{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020"],
		"module": "ES2020",
		"outDir": "./built/",
		"rootDir": "./src/",
		"strict": true,
		"strictPropertyInitialization": false,
		"typeRoots": [
				"node_modules/@types",
				"src/@types"
		],
		"esModuleInterop": true,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": [
	]
 }
